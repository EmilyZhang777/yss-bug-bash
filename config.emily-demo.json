{
  "configVersion": "1",
  "deviceDetected": false,
  "https": true,
  "locales": ["en"],
  "templateVersion": 8,
  "features": [
    {
      "name": "Home",
      "staticPage": {
        "urlTemplate": "url.home",
        "template": "static.home"
      }
    },
    {
      "name": "Geocode IP",
      "geocodeIP": {
        "urlTemplate": "url.geocodeIp"
      }
    },
    {
      "name": "XML Sitemap",
      "xmlsitemap": {
        "urlTemplate": "url.xmlSitemap",
        "blacklist": ["feedback/", "robots.txt"]
      }
    },
    {
      "name": "404 Page",
      "staticPage": {
        "urlTemplate": "url.page_404",
        "template": "static.four_oh_four"
      }
    },
    {
      "name": "Robots.txt",
      "staticPage": {
        "urlTemplate": "url.page_robotstxt",
        "template": "static.robotstxt"
      }
    },
    {
      "name": "State - City Drilldown",
      "entityDirectory": {
        "levels": [
          {
            "urlTemplate": "url.directoryStateList",
            "template": "directory.stateList",
            "field": {
              "name": "address.region"
            },
            "orderBy": {
              "field": {
                "name": "derivedData.address.stateName"
              },
              "order": "ascending"
            }
          },
          {
            "urlTemplate": "url.directoryCityList",
            "template": "directory.cityList",
            "field": {
              "name": "address.city"
            },
            "orderBy": {
              "field": {
                "name": "profile.address.city"
              },
              "order": "ascending"
            }
          },
          {
            "urlTemplate": "url.directoryLocationList",
            "template": "directory.locationList",
            "field": {
              "name": "address.line1"
            },
            "orderBy": {
              "field": {
                "name": "profile.address.line1"
              },
              "order": "ascending"
            }
          }
        ],
        "entityPageSet": "Locations"
      }
    }
  ]
}
